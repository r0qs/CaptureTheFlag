  },
  "license": "ISC",
  "dependencies": {
    "truffle-hdwallet-provider": "^1.0.17",
    "web3": "^1.2.1"
  },
  "devDependencies": {
    "ganache-cli": "^6.7.0-beta.0",
    "truffle": "^5.0.34"
  }
}